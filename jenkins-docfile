pipeline{
    agent any
    stages{
        stage("Git Checkout"){
            steps{
               git credentialsId: 'github', url: 'https://github.com/panda1998/myweb'
                }
            }
        stage("Maven Build"){
            steps{
                sh "mvn clean package"
                 }
            }
        stage("deploy-dev"){
            sshagent(['tomcat-new']) {
             // some block
           }
        }
     }
  }
        
